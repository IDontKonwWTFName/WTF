<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Cross_fence" table="cross_fence">   
	    <composite-id>
		<key-property name="fence_id" type="java.lang.Integer"></key-property>
	    <key-property name="time" type="java.util.Date"></key-property>
	    </composite-id>
	    <property name="In_Out" type="java.lang.Integer"></property>
	</class>  
</hibernate-mapping>  