<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Admin" table="admin">   
	    <id name="user_id" type="java.lang.String">
	    </id>  
	    <property name="password" type="java.lang.String"></property> 
	    <property name="phone" type="java.lang.String"></property>
	    <property name="email" type="java.lang.String"></property>
	    <property name="adress" type="java.lang.String"></property>
	    <property name="unit" type="java.lang.String"></property>
	</class>  
</hibernate-mapping> 