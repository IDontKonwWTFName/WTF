<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="StepNumHistory" table="StepNumHistory">   
	    <id name="id" type="java.lang.Integer">
	     <generator class="native"/>
	    </id>  
	    <property name="shouhuan_id" type="java.lang.String"></property>
	    <property name="time" type="java.util.Date"></property>
	    <property name="stepNum" type="java.lang.Integer"></property>
	</class>  
</hibernate-mapping>  