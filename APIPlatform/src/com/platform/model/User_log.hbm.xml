<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="User_log" table="user_log">   
	    <id name="user_log_id" type="java.lang.Integer">
	    <generator class="native"/>
	    </id>  
	    <property name="log_type" type="java.lang.Integer"></property> 
	    <property name="user_id" type="java.lang.String"></property>
	    <property name="time" type="java.util.Date"></property>
	    <property name="event" type="java.lang.String"></property>
	</class>  
</hibernate-mapping>  