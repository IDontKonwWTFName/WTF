<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Fence" table="fence">   
	    <id name="fence_id" type="java.lang.Integer"><generator class="native"/></id>  
	    <property name="type" type="java.lang.Integer"></property> 
	    <property name="shouhuan_id" type="java.lang.String"></property>
	    <property name="user_id" type="java.lang.String"></property>
	    <property name="fence_name" type="java.lang.String"></property>
	    <property name="fence" type="java.lang.String"></property>
	     <property name="time" type="java.lang.String"></property>
	</class>  
</hibernate-mapping>  