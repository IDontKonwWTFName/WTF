<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Relation" table="relation">
		<composite-id>
		<key-property name="shouhuan_id" type="java.lang.String"></key-property>
		<key-property name="user_id" type="java.lang.String"></key-property>
	    </composite-id>
	    <property name="power" type="java.lang.Integer"></property>
	    <property name="administor" type="java.lang.Integer"></property>
	    <property name="relation" type="java.lang.String"></property>
	</class>  
</hibernate-mapping>  