<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Service" table="service">   
	    <id name="service_type" type="java.lang.Integer"></id>  
	    <property name="service_price" type="java.lang.Integer"></property> 
	    <property name="service_describe" type="java.lang.String"></property>
	    <property name="service_workime" type="java.lang.Integer"></property>
	</class>  
</hibernate-mapping>  