<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.platform.model">  
	<class name="Payment" table="payment">
		<id name="payment_id" type="java.lang.Integer"><generator class="native"/>
		</id>
		<property name="shouhuan_id" type="java.lang.String"></property>
		<property name="user_id" type="java.lang.String"></property>
	    <property name="service_type" type="java.lang.Integer"></property>
	    <property name="begin_time" type="java.util.Date"></property>
	    <property name="end_time" type="java.util.Date"></property>
	</class>  
</hibernate-mapping>  